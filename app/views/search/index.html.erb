<h1><%= @dogs.count %> Results</h1>

<% @dogs.each do |dog| %>
  <div class="dog">
  <div class="image"><%= image_tag(dog.image_url, alt: dog.breed) %></div>
  <p class="breed">Dog Breed: <%= dog.breed %></p>
  <p class="temperament">Temperament: <%= dog.temperament %></p>
  <p class="life_span">Life Span: <%= dog.life_span %></p>
  </div>
<% end %>
